@model  List<WebApplication3.Models.StudentInfo>

@{
    ViewBag.Title = "TestListModel";
}

<h2>TestListModel</h2>

<h2>View @Session["User"]</h2>
<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Mark</th>
    </tr>
    @{
        for (int i = 0; i<Model.Count; i++)
        {
            <tr>
                <td>@Model[i].Id</td>
                <td>@Model[i].Name</td>
                <td>@Model[i].Mark</td>
                <td>@Html.ActionLink("Chỉnh sửa","Edit",new { id = Model[i].Id })</td>
            </tr>
        }
    }
</table>