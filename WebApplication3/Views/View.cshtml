@model  List<WebApplication3.Models.StudentInfo>

@{
    ViewBag.Title = "View";
}

<h2>View</h2>
<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Mark</th>
    </tr>
    @{ 
        for(int i = 0; i<Model.Count; i++)
            {
                <tr>
                    <td>@Model[i].Id</td>
                    <td>@Model[i].Name</td>
                    <td>@Model[i].Mark</td>
                </tr>
            }
    }
</table>
